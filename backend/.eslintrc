{
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier", "only-warn"],
  "ignorePatterns": ["/src/**/*.test.js"],
  "rules": {
    "react/function-component-definition": "off",
    "import/no-extraneous-dependencies": "off",
    "default-param-last": "off",
    "no-console": "warn"
  }
}
